        &lt;img src=&#34;https://HowBoutThemApples.github.io/images/coming-soon.png&#34; alt=&#34;Fruity Pebbles&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;cinnamontoastcrunch2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;cinnamontoastcrunch&#34;&gt;Cinnamon Toast Crunch:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;cinnamontoastcrunch&#34; name=&#34;cinnamontoastcrunch&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/cinnamontoastcrunch.png&#34; alt=&#34;Cinnamon Toast Crunch&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;popcorn2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;popcorn&#34;&gt;Popcorn:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;popcorn&#34; name=&#34;popcorn&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/coming-soon.png&#34; alt=&#34;Popcorn&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;pinacolada2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;pinacolada&#34;&gt;Pina Colada:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;pinacolada&#34; name=&#34;pinacolada&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/pinaColada.png&#34; alt=&#34;Pina Colada&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;cinnamonpecan2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;cinnamonpecan&#34;&gt;Cinnamon Pecan:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;cinnamonpecan&#34; name=&#34;cinnamonpecan&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/coming-soon.png&#34; alt=&#34;Cinnamon Pecan&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;bannanapudding2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;bannanapudding&#34;&gt;Banana Pudding:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;bannanapudding&#34; name=&#34;bannanapudding&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/bananaPudding.png&#34; alt=&#34;Banana Pudding&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;candycane2&#34; data-popularity=&#34;7&#34;&gt;&#xA;            &lt;label for=&#34;candycane&#34;&gt;Candy Cane:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;candycane&#34; name=&#34;candycane&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/candyCane.png&#34; alt=&#34;Candy Cane&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;darkchandbacon2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;darkchandbacon&#34;&gt;Dark Chocolate and Bacon:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;darkchandbacon&#34; name=&#34;darkchandbacon&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/coming-soon.png&#34; alt=&#34;Dark Chocolate and Bacon&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;coconut2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;coconut&#34;&gt;Coconut:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;coconut&#34; name=&#34;coconut&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/coconut.png&#34; alt=&#34;Coconut&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;circusanimalcrackers2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;circusanimalcrackers&#34;&gt;Circus Animal Crackers:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;circusanimalcrackers&#34; name=&#34;circusanimalcrackers&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/coming-soon.png&#34; alt=&#34;Circus Animal Crackers&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;samoa2&#34; data-popularity=&#34;8&#34;&gt;&#xA;            &lt;label for=&#34;samoa&#34;&gt;Samoa:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;samoa&#34; name=&#34;samoa&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/samoa.png&#34; alt=&#34;Samoa&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;chocolatecoveredcaramel2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;chocolatecoveredcaramel&#34;&gt;Chocolate Covered Caramel:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;chocolatecoveredcaramel&#34; name=&#34;chocolatecoveredcaramel&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/chocolatecoveredcaramel.png&#34; alt=&#34;Chocolate Covered Caramel&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;caramel2&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;caramel&#34;&gt;Caramel:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;caramel&#34; name=&#34;caramel&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/caramel.png&#34; alt=&#34;Caramel&#34;&gt;&#xA;          &lt;/div&gt;&#xA;          &lt;div class=&#34;apple-flavor&#34; id=&#34;take52&#34; data-popularity=&#34;5&#34;&gt;&#xA;            &lt;label for=&#34;take5&#34;&gt;Take 5:&lt;/label&gt;&#xA;            &lt;input type=&#34;number&#34; id=&#34;take5&#34; name=&#34;take5&#34; min=&#34;0&#34; value=&#34;0&#34;&gt;&#xA;            &lt;img src=&#34;https://HowBoutThemApples.github.io/images/Apple Pictures/take5.png&#34; alt=&#34;Take 5&#34;&gt;&#xA;          &lt;/div&gt;&#xA;    &lt;/div&gt;&#xA;      &#xA;      &#xA;    &#xA;  &#xA;    &lt;button type=&#34;button&#34; onclick=&#34;calculateOrder()&#34;&gt;Request Purchase&lt;/button&gt;&#xA;  &lt;/form&gt;&#xA;&#xA;&lt;script&gt;&#xA;      &#xA;    function calculateOrder() {&#xA;        var appleFlavors = [&#xA;          { name: &#34;Heath&#34;, id: &#34;heath&#34; },&#xA;          { name: &#34;Oreo&#34;, id: &#34;oreo&#34; },&#xA;          { name: &#34;Apple Pie&#34;, id: &#34;applePie&#34; },&#xA;          { name: &#34;Tagalong&#34;, id: &#34;tagalong&#34; },&#xA;          { name: &#34;Butterfinger&#34;, id: &#34;butterfinger&#34; },&#xA;          { name: &#34;Reese&#39;s&#34;, id: &#34;Reeses&#34; },&#xA;          { name: &#34;Cookies and Cream&#34;, id: &#34;candc&#34; },&#xA;          { name: &#34;Sprinkles&#34;, id: &#34;sprinkles&#34; },&#xA;          { name: &#34;Thin Mint&#34;, id: &#34;thinmint&#34; },&#xA;          { name: &#34;Potato Chip&#34;, id: &#34;potatochip&#34; },&#xA;          { name: &#34;Peanut&#34;, id: &#34;peanut&#34; },&#xA;          { name: &#34;Pecan&#34;, id: &#34;pecan&#34; },&#xA;          { name: &#34;Turtle&#34;, id: &#34;turtle&#34; },&#xA;          { name: &#34;S&#39;mores&#34;, id: &#34;smores&#34; },&#xA;          { name: &#34;Mini Chocolate Chips&#34;, id: &#34;minichocolatechips&#34; },&#xA;          { name: &#34;Mini M&amp;Ms&#34;, id: &#34;minimandms&#34; },&#xA;          { name: &#34;Pretzel&#34;, id: &#34;pretzel&#34; },&#xA;          { name: &#34;Dark Chocolate Sea Salt&#34;, id: &#34;darkchseasalt&#34; },&#xA;          { name: &#34;Chocolate Chip Cookie&#34;, id: &#34;chchipcookie&#34; },&#xA;          { name: &#34;Almond Joy&#34;, id: &#34;almondjoy&#34; },&#xA;          { name: &#34;Mounds&#34;, id: &#34;mounds&#34; },&#xA;          { name: &#34;Pumpkin Spice&#34;, id: &#34;pumpkinspice&#34; },&#xA;          { name: &#34;Snickers&#34;, id: &#34;snickers&#34; },&#xA;          { name: &#34;Strawberry Cheesecake&#34;, id: &#34;strawberrycheesecake&#34; },&#xA;          { name: &#34;Fruity Pebbles&#34;, id: &#34;fruitypebbles&#34; },&#xA;          { name: &#34;Cinnamon Toast Crunch&#34;, id: &#34;cinnamontoastcrunch&#34; },&#xA;          { name: &#34;Popcorn&#34;, id: &#34;popcorn&#34; },&#xA;          { name: &#34;Pina Colada&#34;, id: &#34;pinacolada&#34; },&#xA;          { name: &#34;Cinnamon Pecan&#34;, id: &#34;cinnamonpecan&#34; },&#xA;          { name: &#34;Banana Pudding&#34;, id: &#34;bannanapudding&#34; },&#xA;          { name: &#34;Candy Cane&#34;, id: &#34;candycane&#34; },&#xA;          { name: &#34;Dark Chocolate and Bacon&#34;, id: &#34;darkchandbacon&#34; },&#xA;          { name: &#34;Coconut&#34;, id: &#34;coconut&#34; },&#xA;          { name: &#34;Circus Animal Crackers&#34;, id: &#34;circusanimalcrackers&#34; },&#xA;          { name: &#34;Samoa&#34;, id: &#34;samoa&#34; },&#xA;          { name: &#34;Chocolate Covered Caramel&#34;, id: &#34;chocolatecoveredcaramel&#34; },&#xA;          { name: &#34;Caramel&#34;, id: &#34;caramel&#34; },&#xA;          { name: &#34;Take 5&#34;, id: &#34;take5&#34; }&#xA;        ];&#xA;      &#xA;        var totalApples = 0;&#xA;        var orderDetails = &#39;&#39;;&#xA;      &#xA;        &#xA;        appleFlavors.forEach(function(flavor) {&#xA;          var quantity = parseInt(document.getElementById(flavor.id).value) || 0;&#xA;          totalApples += quantity;&#xA;        });&#xA;      &#xA;        &#xA;        var pricePerApple = totalApples &gt;= 25 ? 8 : 10;&#xA;      &#xA;        &#xA;        appleFlavors.forEach(function(flavor) {&#xA;          var quantity = parseInt(document.getElementById(flavor.id).value) || 0;&#xA;          if (quantity &gt; 0) {&#xA;            var cost = quantity * pricePerApple;&#xA;            orderDetails += `${flavor.name}: ${quantity} @ $${pricePerApple}/apple = $${cost}\n`;&#xA;          }&#xA;        });&#xA;      &#xA;        var estimatedPrice = totalApples * pricePerApple;&#xA;        var shippingRate = 5;&#xA;      &#xA;        var emailBody = `Order Details:\n${orderDetails}\nTotal Apples: ${totalApples}\nTotal Estimated Cost: $${estimatedPrice}\n\n** Free Shipping within a 25 mile radius **`;&#xA;      &#xA;        var mailtoLink = `mailto:enrightsapples@gmail.com?subject=Apple Purchase Request&amp;body=${encodeURIComponent(emailBody)}`;&#xA;        window.location.href = mailtoLink;&#xA;      }&#xA;      &#xA;&#xA;      function sortByPopularity() {&#xA;        var container = document.getElementById(&#39;flavorsContainer&#39;);&#xA;        var appleFlavors = Array.from(container.getElementsByClassName(&#39;apple-flavor&#39;));&#xA;      &#xA;        appleFlavors.sort(function(a, b) {&#xA;          return parseInt(b.dataset.popularity) - parseInt(a.dataset.popularity);&#xA;        });&#xA;      &#xA;        appleFlavors.forEach(function(flavor) {&#xA;          container.appendChild(flavor);&#xA;        });&#xA;      }&#xA;&#xA;    &#xA;    window.onload = sortByPopularity;&#xA;&#xA;&#xA;    function filterFlavors() {&#xA;        var allCheckboxes = document.querySelectorAll(&#39;#flavorCheckboxes input[type=&#34;checkbox&#34;]&#39;);&#xA;      &#xA;        allCheckboxes.forEach(checkbox =&gt; {&#xA;          var flavorDiv = document.querySelector(`#flavorsContainer .apple-flavor[id=&#34;${checkbox.value}&#34;]`);&#xA;          if (checkbox.checked) {&#xA;            flavorDiv.style.display = &#39;block&#39;;&#xA;          } else {&#xA;            flavorDiv.style.display = &#39;none&#39;;&#xA;            &#xA;            var input = flavorDiv.querySelector(&#39;input[type=&#34;number&#34;]&#39;);&#xA;            if (input) {&#xA;              input.value = 0;&#xA;            }&#xA;          }&#xA;        });&#xA;      }      &#xA;      &#xA;      &#xA;      &#xA;&lt;/script&gt;&#xA;&#xA;&lt;style&gt;&#xA;    #flavorsContainer {&#xA;        display: grid;&#xA;        grid-template-columns: repeat(3, 1fr);  &#xA;        gap: 20px;  &#xA;      }&#xA;      &#xA;      .apple-flavor {&#xA;        display: flex;&#xA;        flex-direction: column;&#xA;        align-items: center;&#xA;      }&#xA;      &#xA;      .apple-flavor img {&#xA;        object-fit: cover;&#xA;      }&#xA;      &#xA;      .apple-flavor label,&#xA;      .apple-flavor input {&#xA;        margin-top: 10px;&#xA;      }&#xA;      &#xA;       &#xA;      @media (max-width: 768px) {&#xA;        #flavorsContainer {&#xA;          grid-template-columns: repeat(2, 1fr);  &#xA;        }&#xA;      }&#xA;      &#xA;      @media (max-width: 480px) {&#xA;        #flavorsContainer {&#xA;          grid-template-columns: 1fr;  &#xA;        }&#xA;      }&#xA;      &#xA;      .checkbox-grid {&#xA;        display: grid;&#xA;        grid-template-columns: repeat(3, 1fr);  &#xA;        gap: 10px;  &#xA;        padding: 2rem;&#xA;        background-color: #397642;&#xA;      }&#xA;      &#xA;      &#xA;      &#xA;&lt;/style&gt;</description>
    </item>
